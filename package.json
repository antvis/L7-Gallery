{
  "private": true,
  "name": "L7Gallery",
  "version": "1.0.0",
  "scripts": {
    "start": "dumi dev",
    "docs:build": "dumi build",
    "docs:deploy": "gh-pages -d docs-dist",
    "build": "father build",
    "deploy": "npm run docs:build && npm run docs:deploy",
    "prettier": "prettier --write \"**/*.{js,jsx,tsx,ts,less,md,json}\"",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist"
  ],
  "module": "dist/esm/index.js",
  "typings": "dist/esm/index.d.ts",
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "@antv/l7": "^2.9.21",
    "@antv/l7-maps": "^2.9.21",
    "@antv/larkmap": "^0.3.0",
    "@turf/turf": "^6.5.0",
    "antd": "^4.23.2",
    "color": "^4.2.3",
    "eventemitter3": "^4.0.7",
    "h3-js": "^4.0.0",
    "immer": "^9.0.15",
    "lodash": "^4.17.21",
    "papaparse": "^5.3.2",
    "popmotion": "9.4.2",
    "rc-queue-anim": "^2.0.0",
    "react": "^18.0.0",
    "react-color": "^2.19.3",
    "turf": "^3.0.14",
    "use-immer": "^0.7.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.15.1",
    "@testing-library/react": "^13.0.0",
    "@types/jest": "^27.0.3",
    "@types/lodash": "^4.14.184",
    "@types/papaparse": "^5.3.5",
    "@types/react": "^18.0.18",
    "@umijs/fabric": "^2.8.1",
    "@umijs/test": "^3.0.5",
    "dumi": "^1.1.0",
    "father": "^4.0.0-rc.2",
    "gh-pages": "^3.0.0",
    "lint-staged": "^10.0.7",
    "prettier": "^2.2.1",
    "yorkie": "^2.0.0"
  },
  "license": "MIT"
}
