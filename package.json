{
  "name": "district-data",
  "version": "0.0.4",
  "description": "行政区划数据 SDK",
  "license": "MIT",
  "module": "dist/esm/index.js",
  "browser": "dist/umd/district-data.min.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "father build",
    "build:watch": "father dev",
    "deploy": "npm run docs:build && npm run docs:deploy",
    "dev": "dumi dev",
    "docs:build": "dumi build",
    "docs:deploy": "gh-pages -d docs-dist",
    "doctor": "father doctor",
    "lint": "npm run lint:es && npm run lint:css",
    "lint:css": "stylelint \"{src,test}/**/*.{css,less}\"",
    "lint:es": "eslint \"{src,test}/**/*.{js,jsx,ts,tsx}\"",
    "prepare": "husky install && dumi setup",
    "prepublishOnly": "father doctor && npm run build",
    "start": "npm run dev"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.{md,json}": [
      "prettier --write --no-error-on-unmatched-pattern"
    ],
    "{src,test}/**/*.{css,less}": [
      "stylelint --fix",
      "prettier --write"
    ],
    "*.{js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@turf/helpers": "^6.5.0",
    "@turf/simplify": "^6.5.0",
    "geobuf": "^3.0.2",
    "pbf": "^3.2.1"
  },
  "devDependencies": {
    "@ant-design/icons": "^5.0.1",
    "@antv/l7": "^2.13.9",
    "@antv/larkmap": "^1.3.1",
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@types/geobuf": "^3.0.1",
    "@types/pbf": "^3.0.2",
    "@umijs/lint": "^4.0.0",
    "antd": "^5.2.2",
    "dumi": "^2.1.14",
    "eslint": "^8.23.0",
    "father": "^4.1.0",
    "geojson2svg": "^1.3.3",
    "gh-pages": "^5.0.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "parse-svg": "^0.0.0",
    "prettier": "^2.7.1",
    "prettier-plugin-organize-imports": "^3.0.0",
    "prettier-plugin-packagejson": "^2.2.18",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "shp-write": "git+https://github.com/transportforcairo/shp-write.git",
    "stylelint": "^14.9.1",
    "tokml": "^0.4.0",
    "topojson": "^3.0.2"
  },
  "peerDependencies": {
    "react": ">=16.9.0",
    "react-dom": ">=16.9.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "authors": []
}
